#include <stdio.h>
#include <math.h>

int prime(int n);

void main(){
    int i=2,j,x[100],k=0;
    
    while(k<100){
        if (prime(i)==1){
            x[k]=i;
            k++;
        }
        i++;
    }
    
    for(j=0;j<k;j++){
        printf("Prime %d: %d \n",j+1,x[j]);
   }
}

int prime(int n){
    int flag=1;
    for(int i=2;i<sqrt(n)+1;i++){
        if(n%i==0){
            flag=0;
            break;
        }
    }
    if(n==2)
        flag=1;
    
    return flag;
}
