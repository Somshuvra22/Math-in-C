#include <stdio.h>
#include <math.h>

int prime(int n);

void main() {
    int i,x[100],k=0,j;
    
    for(i=2;i<=100;i++){
        if (prime(i)==1){
            x[k]=i;
            k++;
        }
    }
   for(j=0;j<k;j++){
       printf("%d \n",x[j]);
   }
}

int prime(int n){
    int flag=1;
    for(int i=2;i<sqrt(n)+1;i++){
        if(n%i==0)
            flag=0;
    }
    return flag;
}
