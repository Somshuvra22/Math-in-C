#include <stdio.h>
#include <math.h>

int main() {
    int n,i,flag=1;
    printf("Number: ");
    scanf("%d",&n);
    
    for(i=2;i<sqrt(n)+1;i++){
        if(n%i==0){
            flag=0;
        }
    }
    
    if(flag==0){
        printf("Not a Prime Number");
    }else{
        printf("Prime Number");
    }

    return 0;
}
